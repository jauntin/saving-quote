{
    "name": "jauntin/saving-quote",
    "description": "Laravel package for saving user quote",
    "keywords": [
        "jauntin",
        "saving-quote"
    ],
    "homepage": "https://github.com/jauntin/saving-quote",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Daryna Syvyuk",
            "email": "daryna@jauntin.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.5",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Jauntin\\SavingQuote\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "analyze": "vendor/bin/phpcs && vendor/bin/phpstan analyse --memory-limit=1G",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jauntin\\SavingQuote\\SavingQuoteServiceProvider"
            ]
        }
    }
}
